citHeader("To cite hogwash in publications use:")

citEntry(entry = "Article",
  title        = "Paper title",
  author       = personList(as.person("Katie Saund"), 
                    as.person("Other author name"),
                    as.perons("Other author name")),
  journal      = "Journal Name",
  year         = "Num",
  volume       = "Num",
  number       = "Num",
  pages        = "Num--Num",
  url          = "https://www.linkhere.com/",

  textVersion  =
  paste("Katie Saund (2019).",
        "Paper title",
        "Journal, Volume(Issue), PageNum-PageNum.",
        "URL https://www.linkhere.com/.")
)
